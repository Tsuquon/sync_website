            <time class="c-meta__item c-meta__item--block-at-lg" datetime="2022-08-01" itemprop="datePublished">01 Aug 2022</time>
        
        
            
                
                    
                        
                            <div class="c-meta__item c-meta__item--block-at-lg u-text-bold" data-test="journal-title-and-link">Nature Physics</div>
                        
                    
                
            
            
